{
  "version": "1.3.1",
  "name": "mockbin",
  "description": "Mock, Test & Track HTTP calls",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "homepage": "http://mockbin.com/",
  "license": "MIT",
  "main": "./src/index.js",
  "bin": "./bin/mockbin",
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmadnassri/mockbin.git"
  },
  "bugs": {
    "url": "https://github.com/ahmadnassri/mockbin/issues"
  },
  "keywords": [
    "API",
    "Bin",
    "HAR",
    "HTTP",
    "JSON",
    "Mock",
    "Mocking",
    "Test",
    "Testing"
  ],
  "config": {
    "port": 8080,
    "redis": "redis://127.0.0.1:6379",
    "quiet": false,
    "blanket": {
      "pattern": "src"
    }
  },
  "scripts": {
    "test": "standard && mocha --recursive --reporter spec",
    "coverage": "mocha -r blanket -R mocha-lcov-reporter > test/report/lcov.info",
    "html-cov": "mocha -r blanket -R html-cov > test/report/coverage.html",
    "codeclimate": "codeclimate < test/report/lcov.info"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "codeclimate-test-reporter": "0.0.4",
    "mocha": "^2.2.1",
    "mocha-lcov-reporter": "0.0.2",
    "should": "^5.1.0",
    "standard": "^3.1.2",
    "unirest": "^0.4.0"
  },
  "dependencies": {
    "change-case": "^2.2.0",
    "commander": "^2.6.0",
    "compression": "^1.4.1",
    "cookie-parser": "^1.3.4",
    "debug": "^2.1.2",
    "dicer": "^0.2.3",
    "express": "^4.12.2",
    "har-validator": "^1.1.2",
    "jade": "^1.9.2",
    "marked": "^0.3.3",
    "media-typer": "^0.3.0",
    "method-override": "^2.3.1",
    "moment": "^2.9.0",
    "morgan": "^1.5.1",
    "node-uuid": "^1.4.2",
    "qs": "^2.3.3",
    "rc": "^0.6.0",
    "redis": "^0.12.1",
    "require-directory": "^2.1.0",
    "xmlbuilder": "^2.6.1",
    "yamljs": "^0.2.1"
  }
}
